@inherits Umbraco.Web.Mvc.UmbracoTemplatePage
@{
	var fiveFinishedProjects = CurrentPage.Site().FirstChild("projectsMain").Children("projectItem").Where("Visible && projectStatus == @0",9).OrderBy("CreateDate desc").Take(3);
} <ul style="list-style:none;">
	@foreach(var item in fiveFinishedProjects){
		<li>
			<u><a href="@item.Url">@item.Name</a></u>
		</li>
	}
<ul>
	